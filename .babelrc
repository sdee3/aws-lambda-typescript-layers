{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./functions/src"],
        "alias": {
          "aws-sdk/client-s3": "./layers/aws-deps/nodejs/node_modules/@aws-sdk/client-s3",
          "fbx2gltf": "./layers/fbx2gltf/nodejs/*",
          "three": "./layers/three/nodejs/node_modules/three"
        }
      }
    ]
  ]
}
